enum Color : dword
{
    CornflowerBlue = 1,
    Black = 123,
    Test = 2,
}

enum Type : word
{
    Something = 1,
    Foobar = 2,
}

data Head
{
    uint8 id;
    uint16 size;
    optional uint32 crc;
}

data Complex
{
    Color color;
    Type type;
}

message TheMessage
{
    Head head;
    repeated Complex[2] complex;
}